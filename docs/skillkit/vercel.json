{
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- .",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api.html"
    },
    {
      "source": "/_next/:path*",
      "destination": "https://skillkit-docs.vercel.app/_next/:path*"
    },
    {
      "source": "/docs/:path*",
      "destination": "https://skillkit-docs.vercel.app/docs/:path*"
    },
    {
      "source": "/docs",
      "destination": "https://skillkit-docs.vercel.app/docs"
    }
  ]
}
